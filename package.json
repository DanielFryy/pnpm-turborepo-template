{
  "name": "my-turborepo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "check-format": "prettier -c ./**/*.{js,ts,tsx,json,css,md}",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,css,md}\"",
    "check-types": "turbo run check-types",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "cz": "cz",
    "update-dependencies": "sh ncu.sh",
    "clean": "rm -rf node_modules **/**/node_modules **/**/.next **/**/.turbo",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/types": "^19.8.1",
    "commitizen": "^4.3.1",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "catalog:"
  },
  "packageManager": "pnpm@10.11.1",
  "engines": {
    "node": ">=18"
  }
}
